  "main": "index.js",
  "scripts": {
    "pre-commit": "lint-staged",
    "build": "tsc -p .",
    "format": "yarn prettier && yarn eslint",
    "lint": "eslint src/ --ext .ts --cache --fix",
    "start": "node -r dotenv/config dist/index.js",
    "prettier": "prettier --write \"src/**/*\" && prettier-package-json --write",
    "type": "tsc --noEmit",
    "watch": "ts-node-dev -r dotenv/config --respawn --transpileOnly src/index.ts"
  },
  "dependencies": {
    "aws-sdk": "^2.747.0",
    "bytes": "^3.1.0",
    "convict": "^6.0.0",
    "cors": "^2.8.5",
    "cron": "^1.8.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-ws": "^4.0.0",
    "lodash": "^4.17.20",
    "redis": "^3.0.2",
    "uuid": "^8.3.0",
    "ws": "^7.3.1"
  },
  "devDependencies": {
    "@types/bytes": "^3.1.0",
    "@types/convict": "^5.2.1",
    "@types/cors": "^2.8.7",
    "@types/cron": "^1.7.2",
    "@types/express": "^4.17.8",
    "@types/express-ws": "^3.0.0",
    "@types/lodash": "^4.14.161",
    "@types/redis": "^2.8.27",
    "@types/uuid": "^8.3.0",
    "@types/ws": "^7.2.6",
    "@typescript-eslint/eslint-plugin": "^4.1.0",
    "@typescript-eslint/parser": "^4.1.0",
    "eslint": "^7.8.1",
    "eslint-config-airbnb-typescript": "^10.0.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.4",
    "lint-staged": "^10.3.0",
    "prettier": "^2.1.1",
    "ts-node": "^9.0.0",
    "ts-node-dev": "^1.0.0-pre.62",
    "typescript": "^4.0.2"
  }
}
