    "uuid": "^7.0.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.6",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.3",
    "@types/lodash": "^4.14.149",
    "@types/multer": "^1.4.2",
    "@types/multer-s3": "^2.7.7",
    "@types/socket.io": "^2.1.4",
