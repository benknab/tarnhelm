	"scripts": {
		"build": "run-s next:build next:export",
		"check:format": "prettier --check .",
		"check:lint": "eslint --cache src",
		"check:types": "tsc --noEmit",
		"checks": "run-p --aggregate-output --continue-on-error check:*",
		"dev": "next dev",
		"fix": "run-s --continue-on-error fix:lint fix:format",
		"fix:format": "prettier --write .",
		"fix:lint": "yarn check:lint --fix",
		"next:build": "next build",
		"next:export": "next export -o _static",
		"next:start": "next start -H 0.0.0.0 -p ${PORT:-8080}",
		"pre-commit": "lint-staged --concurrent false",
		"start": "yarn next:start",
