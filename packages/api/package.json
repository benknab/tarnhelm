	"scripts": {
		"build": "tsc -p .",
		"check:format": "prettier --check .",
		"check:lint": "eslint --cache src",
		"check:types": "tsc --noEmit",
		"checks": "run-p --aggregate-output --continue-on-error check:*",
		"dev": "ts-node-dev -r dotenv/config --respawn --transpile-only src/index.ts",
		"fix": "run-s --continue-on-error fix:lint fix:format",
		"fix:format": "prettier --write .",
		"fix:lint": "yarn check:lint --fix",
		"postinstall": "patch-package",
		"pre-commit": "lint-staged --concurrent false",
		"start": "node -r dotenv/config dist/index.js",
		"test": "echo TODO",
		"type": "tsc --noEmit"
	},
